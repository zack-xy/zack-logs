    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/figlet": "^1.7.0",
    "@types/node": "^24.0.14",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
